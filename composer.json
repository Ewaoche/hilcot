{
    "name": "ewaoche/hilcot",
    "description": "Hilcot Energy Services",
    "type": "project",
    "version": "2.0.0",
    "keywords": ["psr4", "template"],
    "license": "BSD",
    "authors": [{
        "name": "Ewaoche",
        "email": "info@hilcot.com",
        "homepage": "https://hilcot.com/",
        "role": "Developer"
    }],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "classmap": ["_apps/", "_apps/Apps/"],
        "psr-4": {
            "Apps\\": ["_apps/", "_apps/Apps/"]
        }
    },
    "require": {
        "php": ">=5.6.36",
        "verot/class.upload.php": "dev-master"
    },
    "scripts": {},
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["application"]
}
